    <aside
      data-match-modal
      class="hidden fixed inset-0 bg-gray-500 bg-opacity-50 flex items-center justify-center"
      role="dialog"
      aria-labelledby="match-modal-title"
    >
      <div class="bg-white rounded p-6 w-full max-w-2xl">
        <h2 id="match-modal-title" class="text-2xl mb-4">Match Found!</h2>
        <section data-match-restaurant-info>
          <!-- Restaurant information will be displayed here -->
        </section>
      </div>
    </aside>

